        <partial name="_NavBarAdminPartial.cshtml" />
    }
    else
    {
       
        <partial name="_NavbarStudentAndTeacherPartial.cshtml" />
    }
}

else
{
    <partial name="_NavBarNotLoggedInPartial.cshtml" />

}


